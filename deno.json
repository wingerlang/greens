    "lucide-react": "npm:lucide-react@^0.469.0",
    "@dnd-kit/core": "npm:@dnd-kit/core@^6.1.0",
    "@dnd-kit/sortable": "npm:@dnd-kit/sortable@^8.0.0",
    "@dnd-kit/utilities": "npm:@dnd-kit/utilities@^3.2.2",
    "zod": "npm:zod@^3.23.8",
